{
  "name": "bienes_raices",
  "version": "1.0.0",
  "description": "Aplicacion Bienes Raices",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./index.js",
    "server": "nodemon ./index.js",
    "css": "postcss public/css/tailwind.css -o public/css/app.css --watch",
    "js": "webpack --watch",
    "dev": "concurrently \"npm run css\" \"npm run js\" ",
    "db:import": "node ./seed/seeders.js -i",
    "db:delete": "node ./seed/seeders.js -d"
  },
  "keywords": [
    "Mysql",
    "Express",
    "Tailwindcss"
  ],
  "author": "Sandra Milena Villamizar Suarez",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "dropzone": "^5.9.3",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "nodemailer": "^6.9.11",
    "pug": "^3.0.2",
    "sequelize": "^6.37.1",
    "tiny-csrf": "^1.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
